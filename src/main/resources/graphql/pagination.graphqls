type Pagination {
    pageActuelle: Int
    pageMin: Int
    pageMax: Int
    total: Long
    produits: [Produit]!
    categories: [Categorie]!
}

extend type Query {
    pagination(type: String!, page: Int!, npp: Int!, nom: String): Pagination!
}

schema {
    query: Query
    mutation: Mutation
}

